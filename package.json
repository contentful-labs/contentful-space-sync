{
  "name": "contentful-space-sync",
  "version": "1.2.3",
  "description": "Syncs Contentful spaces",
  "bin": "bin/space-sync",
  "scripts": {
    "build": "rimraf dist && mkdirp dist && babel lib --out-dir dist",
    "clean": "rimraf dist && rimraf coverage",
    "prepublish": "npm run build",
    "pretest": "standard",
    "test": "npm run test:cover",
    "test:cover": "isparta cover test/runner",
    "test:only": "./test/runner",
    "browser-coverage": "npm run test:cover && opener coverage/lcov-report/index.html"
  },
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "contentful": "^1.2.0",
    "bluebird": "^3.0.5",
    "contentful-management": "^0.5.0",
    "lodash": "^3.10.1",
    "npmlog": "^2.0.0",
    "yargs": "^3.27.0"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.3",
    "babel-plugin-rewire": "^0.1.22",
    "blue-tape": "^0.1.10",
    "glob": "^5.0.15",
    "mkdirp": "^0.5.1",
    "opener": "^1.4.1",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "standard": "^5.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful-labs/contentful-space-sync.git"
  },
  "keywords": [
    "contentful",
    "sync",
    "space"
  ],
  "author": "Tiago Rodrigues <tiago@contentful.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contentful-labs/contentful-space-sync/issues"
  },
  "homepage": "https://github.com/contentful-labs/contentful-space-sync#readme",
  "standard": {
    "parser": "babel-eslint"
  }
}
